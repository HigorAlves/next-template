{
  "name": "nata-next",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next dev:tsm",
    "dev:next": "next dev",
    "dev:tsm": "tsm src --exportType default --nameFormat none --quoteType single --watch",
    "start": "next start",
    "test": "jest -o --silent",
    "test:watch": "jest -o --watch --silent",
    "test:coverage": "jest -u --ci --silent --coverage",
    "test:ci": "jest -u --ci --silent --verbose --passWithNoTests",
    "build:next": "next build",
    "build:functions": "cd \"functions\" && yarn",
    "build": "yarn build:next && yarn build:functions",
    "predeploy": "yarn build && mv -f .next/ functions/.next",
    "postdeploy": "rm -rf functions/.next",
    "deploy": "firebase deploy",
    "serve:firebase": "firebase serve --only functions,hosting",
    "serve:coverage": "open http://localhost:5000/ && serve coverage/lcov-report",
    "prettier": "npx prettier --write src/**/*.{ts,tsx,json}",
    "lint": "tsc --noEmit && eslint --fix --ext ts,tsx .",
    "release": "standard-version",
    "release:major": "yarn run release --release-as major --no-verify",
    "release:minor": "yarn run release --release-as minor --no-verify",
    "release:patch": "yarn run release --release-as patch --no-verify",
    "release:alpha": "yarn run release --prerelease alpha --no-verify",
    "release:beta": "yarn run release --prerelease beta --no-verify",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "lighthouse": "lighthouse http://localhost:3000 --chrome-flags='--no-sandbox --headless --disable-gpu' --output html --output-path ./lighthouse-report.html --view"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,json}": [
      "pretty-quick --staged"
    ],
    "*.{tsx,ts}": [
      "yarn run lint"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "globby": "^11.0.1",
    "next": "10.0.3",
    "node-sass": "^5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reinspect": "^1.1.0",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/react": "^6.1.9",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lighthouse": "^6.5.0",
    "lint-staged": "^10.5.2",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^11.3.2",
    "standard-version": "^9.0.0",
    "typed-scss-modules": "^3.4.1",
    "typescript": "^4.1.2",
    "webp-loader": "^0.6.0"
  }
}
